{
    "name": "topface/elastica",
    "description": "Elasticsearch Client",
    "keywords": ["search","client"],
    "homepage": "http://elastica.io/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Ruflin",
            "homepage": "http://ruflin.com/"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/log": "^2.0|^3.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "aws/aws-sdk-php": "*",
        "guzzlehttp/guzzle": "*"
    },
    "suggest": {
        "aws/aws-sdk-php": "Allow using IAM authentication with Amazon ElasticSearch Service",
        "egeloen/http-adapter": "Allow using httpadapter transport",
        "guzzlehttp/guzzle": "Allow using guzzle 6 as the http transport",
        "monolog/monolog": "Logging request"
    },
    "autoload": {
        "psr-4": {
            "Elastica6\\": "lib/Elastica6/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elastica6\\Test\\": "test/Elastica6/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}
